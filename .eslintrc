{
  "rules": {
    "no-console": 0,
    "no-underscore-dangle": 0,
    "no-unused-vars": ["error", { "argsIgnorePattern": "next" }],
    "no-use-before-define": ["error", { "variables": false }],
    "no-multi-str": 0,
    "no-await-in-loop": 0,
    "linebreak-style": 0,
    "prefer-destructuring": 0,
    "no-return-assign": 0,
    "no-plusplus": 0,
    "no-restricted-syntax": 0
  },
  "env": {
    "node": true,
    "mocha": true,
    "es6": true
  },
  "parserOptions": {
    "ecmaVersion": 2018,
    "ecmaFeatures.experimentalObjectRestSpread": true
  },
  "extends": [
    "airbnb-base"
  ]
}